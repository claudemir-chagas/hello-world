{
  "extends": "airbnb",
  "rules": {
    "import/order": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-control-regex": 0,
    "no-await-in-loop": 0,
    "arrow-parens": [2, "as-needed", { "requireForBlockBody": true }],
    "prefer-object-spread": 0,
    "react/no-this-in-sfc": 0,
    "react/jsx-indent": 0,
  },
  "plugins": ["json"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true,
    },
    "sourceType": "module"
  },
  "env": {
    "node": true
  }
}